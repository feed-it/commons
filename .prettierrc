{
	"trailingComma": "es5",
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"bracketSameLine": false,
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"proseWrap": "never",
	"useTabs": true,
	"arrowParens": "always",
	"singleAttributePerLine": true,
	"printWidth": 120,
	"overrides": [
		{
			"files": "*.scss",
			"options": {
				"singleQuote": false
			}
		}
	]
}
